{
  "name": "capsule-editor",
  "version": "0.9.13",
  "module": "./src/index.js",
  "main": "./dist/CapsuleEditor.umd.min.js",
  "scripts": {
    "commit": "git add -A && git commit -m 'Pre-release commit';",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name CapsuleEditor ./src/Editor.vue",
    "build-dev-lib": "NODE_ENV=development vue-cli-service build --target lib --name CapsuleEditor ./src/Editor.vue",
    "lint": "vue-cli-service lint",
    "release-patch": "yarn run build && yarn run commit && npm version patch; yarn run release;",
    "release-minor": "yarn run build && yarn run commit && npm version minor; yarn run release;",
    "release-major": "yarn run build && yarn run commit && npm version major; yarn run release;",
    "release": "yarn run commit; yarn publish; git push --follow-tags;"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "animate.css": "^3.7.2",
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "capsule-common": "^0.1.21",
    "codemirror": "^5.52.2",
    "js-beautify": "^1.10.3",
    "popper.js": "^1.16.1",
    "vue-interface": "^0.4.55"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.3",
    "@vue/cli-plugin-eslint": "^4.2.3",
    "@vue/cli-service": "^4.2.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "html-beautify-webpack-plugin": "^1.0.5",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11",
    "webpack-manifest-plugin": "^2.2.0"
  }
}
