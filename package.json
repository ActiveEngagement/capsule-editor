{
  "name": "capsule-editor",
  "version": "0.9.1",
  "module": "./src/index.js",
  "main": "./dist/CapsuleEditor.umd.min.js",
  "scripts": {
    "commit": "git add -A && git commit -m 'Pre-release commit';",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name CapsuleEditor ./src/index.js",
    "build-dev-lib": "NODE_ENV=development vue-cli-service build --target lib --name CapsuleEditor ./src/index.js",
    "lint": "vue-cli-service lint",
    "release-patch": "npm run build && npm run commit && npm version patch; npm run release;",
    "release-minor": "npm run build && npm run commit && npm version minor; npm run release;",
    "release-major": "npm run build && npm run commit && npm version major; npm run release;",
    "release": "npm run commit; npm publish; git push --follow-tags;"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "animate.css": "^3.7.2",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "codemirror": "^5.48.0",
    "js-beautify": "^1.10.0",
    "popper.js": "^1.15.0",
    "vue-interface": "^0.4.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.2",
    "@vue/cli-plugin-eslint": "^3.9.2",
    "@vue/cli-service": "^3.9.2",
    "babel-eslint": "^10.0.2",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.7.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.3",
    "html-beautify-webpack-plugin": "^1.0.5",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "webpack-manifest-plugin": "^2.0.4"
  }
}
