{
  "name": "capsule-editor",
  "version": "1.0.0-beta.3",
  "module": "./src/index.js",
  "main": "./dist/CapsuleEditor.umd.min.js",
  "scripts": {
    "commit": "git add -A && git commit -m 'Pre-release commit';",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name CapsuleEditor ./src/Editor.vue",
    "build-dev-lib": "NODE_ENV=development vue-cli-service build --target lib --name CapsuleEditor ./src/Editor.vue",
    "lint": "vue-cli-service lint",
    "release-patch": "npm run build && npm run commit && npm version patch; npm run release;",
    "release-patch-beta": "npm run build && npm run commit && npm version prepatch --preid beta; npm run release;",
    "release-minor": "npm run build && npm run commit && npm version minor; npm run release;",
    "release-minor-beta": "npm run build && npm run commit && npm version preminor --preid beta; npm run release;",
    "release-major": "npm run build && npm run commit && npm version major; npm run release;",
    "release-major-beta": "npm run build && npm run commit && npm version premajor --preid beta; npm run release;",
    "release": "npm run commit; npm publish; git push --follow-tags;",
    "prerelease": "npm run build && npm run commit && npm version prerelease; npm run release;"
  },
  "dependencies": {
    "@codemirror/basic-setup": "^0.19.0",
    "@codemirror/lang-html": "^0.19.1",
    "@codemirror/panel": "^0.19.0",
    "@codemirror/state": "^0.19.1",
    "@codemirror/theme-one-dark": "^0.19.0",
    "@codemirror/view": "^0.19.4",
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/vue-fontawesome": "^2.0.0",
    "@vue-interface/animate-css": "^0.9.4",
    "@vue-interface/badge": "^0.9.3",
    "@vue-interface/btn": "^0.9.9",
    "@vue-interface/btn-activity": "^0.9.5",
    "@vue-interface/btn-dropdown": "^0.9.13",
    "@vue-interface/form-control": "^0.10.3",
    "@vue-interface/navbar": "^0.9.6",
    "@vue-interface/slide-deck": "^0.9.6",
    "@vue-interface/utils": "^0.9.23",
    "axios": "^0.21.0",
    "bootstrap": "^4.5.2",
    "capsule-common": "^0.7.0",
    "capsule-lint": "^0.1.0",
    "codemirror": "^5.58.0",
    "js-beautify": "^1.13.0",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.6",
    "@vue/cli-service": "^4.5.6",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.9.0",
    "eslint-config-standard": "^16.0",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0",
    "html-beautify-webpack-plugin": "^1.0.5",
    "node-sass": "^5.0",
    "sass-loader": "^10.0",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.0.0"
  }
}
